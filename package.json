{
  "name": "ergosum-cli",
  "version": "0.4.0",
  "description": "Simple AI-friendly CLI for ErgoSum memory management. Perfect for Claude and other AI assistants.",
  "main": "dist/cli.js",
  "bin": {
    "ergosum": "dist/cli.js",
    "egs": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run typecheck",
    "prepack": "npm run build"
  },
  "keywords": [
    "ai",
    "memory",
    "cli",
    "claude",
    "assistant",
    "ergosum",
    "simple"
  ],
  "author": "ErgoSum",
  "license": "MIT",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@types/open": "^6.1.0",
    "commander": "^11.1.0",
    "configstore": "^7.0.0",
    "node-fetch": "^3.3.2",
    "open": "^8.4.2"
  },
  "devDependencies": {
    "@types/configstore": "^6.0.2",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TwoErgoSum/ErgoSum-cli.git"
  },
  "bugs": {
    "url": "https://github.com/TwoErgoSum/ErgoSum-cli/issues"
  },
  "homepage": "https://github.com/TwoErgoSum/ErgoSum-cli#readme"
}
