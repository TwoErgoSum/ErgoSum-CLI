{
  "name": "ergosum-cli",
  "version": "0.1.0",
  "description": "CLI tool for integrating ErgoSum memory and context with AI tools (Claude Code, Codex, Gemini CLI, etc.)",
  "main": "dist/cli.js",
  "bin": {
    "ergosum": "dist/cli.js",
    "egs": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run typecheck",
    "prepack": "npm run build"
  },
  "keywords": [
    "ai",
    "claude",
    "codex",
    "gemini",
    "cli",
    "memory",
    "context",
    "ergosum"
  ],
  "author": "ErgoSum",
  "license": "MIT",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@types/open": "^6.1.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "configstore": "^7.0.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "node-fetch": "^3.3.2",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/configstore": "^6.0.2",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TwoErgoSum/ErgoSum-cli.git"
  },
  "bugs": {
    "url": "https://github.com/TwoErgoSum/ErgoSum-cli/issues"
  },
  "homepage": "https://github.com/TwoErgoSum/ErgoSum-cli#readme"
}
